<h1>Welcome,
  <%= @user.user_name %>
</h1>

<%= link_to  "Update My Information", edit_user_path(@user) %>

  <h3>Account Information</h3>
  <p>First Name:
    <%= @user.first_name %>
  </p>
  <p>Last Name:
    <%= @user.last_name %>
  </p>
  <p>Address:
    <%= @user.street_address %>
  </p>
  <p>City:
    <%= @user.city %>
  </p>
  <p>State:
    <%= @user.state %>
  </p>
  <p>Zip Code:
    <%= @user.zip %>
  </p>