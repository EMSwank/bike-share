<h1>Trips</h1>

<table>
  <% @trips.each do |trip| %>
  <tr>
    <td>Duration</td>
    <td>Start Date</td>
    <td>Start Station</td>
    <td>End Date</td>
    <td>End Station</td>
    <td>Bike</td>
    <td>Subscription Type</td>
    <td>Zip Code</td>
  </tr>
  <tr>
    <td>trip.duration</td>
    <td>trip.start_date</td>
    <td>trip.start_station</td>
    <td>trip.end_date</td>
    <td>trip.end_station</td>
    <td>trip.bike_id</td>
    <td>trip.bike_id</td>
    <td>trip.subscription_type</td>
    <td>trip.zip_code</td>
  </tr>
  <% end %>
</table>